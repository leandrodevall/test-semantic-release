{
  "name": "@monoleasa/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve --project monoleasa",
    "lint:bar": "nx lint --project bar",
    "lint:baz": "nx lint --project baz",
    "lint:foo": "nx lint --project foo",
    "lint": "nx run-many --target=lint --all",
    "test:affected": "npx nx affected -t test",
    "test:bar": "nx test  --project bar",
    "test:baz": "nx test  --project baz",
    "test:foo": "nx test --project foo",
    "test": "nx run-many --target=test --all",
    "build:bar": "nx build  --project bar",
    "build:baz": "nx build  --project baz",
    "build:foo": "nx build --project foo",
    "build": "nx run-many --target=build --all",
    "bump-version:bar": "rjp libs/bar/package.json version $VERSION",
    "bump-version:baz": "rjp libs/baz/package.json version $VERSION",
    "bump-version:foo": "rjp libs/foo/package.json version $VERSION",
    "update-deps": "PACKAGE_NAME=$PACKAGE_NAME VERSION=$VERSION npx nx run-many -t update-deps"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~16.1.0",
