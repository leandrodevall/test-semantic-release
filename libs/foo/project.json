{
  "name": "foo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/foo/src",
  "prefix": "monoleasa",
  "tags": [],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": [
        "{workspaceRoot}/dist/{projectRoot}"
      ],
      "options": {
        "project": "libs/foo/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/foo/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/foo/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/foo/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/foo/**/*.ts",
          "libs/foo/**/*.html"
        ]
      }
    },
    "release": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx semantic-release --debug --extends=./libs/foo/.release.config.js"
      }
    },
    "update-deps": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx rjp ./dist/libs/foo/package.json $PACKAGE_NAME $VERSION"
      }
    }
  }
}
